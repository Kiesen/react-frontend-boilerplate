{
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "emmet.syntaxProfiles": {
    "javascript": "jsx"
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "eslint.alwaysShowStatus": true,
  "eslint.autoFixOnSave": false,
  "prettier.eslintIntegration": true,
  "explorer.confirmDragAndDrop": false,
  "files.associations": {
    "*.js": "javascriptreact"
  },
  "files.autoSave": "onFocusChange",
}